<UserControl x:Class="WpfApplication2.Views.NewUserControl"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
			 xmlns:local="clr-namespace:WpfApplication2.Views"
			  xmlns:diag="clr-namespace:System.Diagnostics;assembly=WindowsBase"
			 mc:Ignorable="d" 
			 d:DesignHeight="300" d:DesignWidth="300">
	<Grid>
		<Button x:Name="button" Content="Press me" HorizontalAlignment="Left" Margin="139,125,0,0" VerticalAlignment="Top" Width="75"/>
		<TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="139,175,0,0" TextWrapping="Wrap" Text="{Binding TestString}" VerticalAlignment="Top" Width="120"/>
	</Grid>
</UserControl>
